{
  "name": "landrepo",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.14.0",
    "yarn": "please-use-npm",
    "npm": ">=8.3.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:packages": "turbo run build --filter=@project/*",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx}'",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx}'"
  },
  "devDependencies": {
    "@project/eslint-config-custom": "*",
    "eslint": "^8.26.0",
    "prettier": "^2.7.1",
    "turbo": "^1.6.1"
  }
}
